{
    "schemaVersion": 1,
    "id": "dndclasses",
    "version": "${version}",

    "name": "DND Classes",
    "description": "Adds DND classes to minecraft.",
    "authors": [
        "mattonfire"
    ],
    "contact": {
        "homepage": "https://github.io/mcrrobinson"
    },

    "license": "CC0-1.0",
    "icon": "assets/dndclasses/icon.png",

    "environment": "*",
    "entrypoints": {
        "main": [
            "mattonfire.dnd.classes.DnDClasses"
        ],
        "client": [
            "mattonfire.dnd.classes.DoubleJump.DoublejumpClient",
            "mattonfire.dnd.classes.PowerupKeybind.PowerupKeybindClient",
            "mattonfire.dnd.classes.gui.GUIClient"
        ]
    },
    "mixins": [
        "dndclasses.mixins.json"
    ],

    "depends": {
        "fabricloader": ">=0.7.4",
        "fabric": "*",
        "minecraft": "1.16.x"
    },
    "suggests": {
        "another-mod": "*"
    }
}